      .content
        - i=0
        form(method="POST" action="/add")
          each val in data
            .container-fluid
              .row
                .col-md-12
                  .card
                    .header

                      h4.title #{tablenames[i]}
                        -if(cond == 'table'){
                          <input class="btn btn-primary gg" type="submit" id="#{tablenames[i]}" name="#{tablenames[i]}" value="VIEW">
                          <input class="btn btn-primary" type="submit" id="#{tablenames[i]}" name="#{tablenames[i]}" value="ADD">
                        -} else{
                          <input class="btn btn-primary" type="submit" id="#{tablenames[i]}" name="#{tablenames[i]}" value="ADD">
                          .search-container
                              select.form-select(aria-label='Default select example',id="selectbox",placeholder="search",name='selectbox')
                                option(selected='') Search By
                                  |   
                                  

                              input(type='text', placeholder='Search..', name='search',class="hamza",id="search")
                              |       
                              button(type='submit',class="btn btn-primary",id="btnsearch")
                                i.fa.fa-search
                          
                        -}

                      - i +=1
                      |                                 
                      p.category Here is a subtitle for this table
                    |                             
                    .content.table-responsive.table-full-width
                      table.table.table-hover.table-striped.table-bordered
                        thead
                          each vals,key in val[0]
                            th.rw #{key}         &#x9;    

                        -if( cond =='table'){
                          tbody
                            - p = 0
                            each item in val
                              -p+=1
                              -if(p === 5){
                                - break;
                              -}
                              tr 
                                each vals,key in item 

                                  td #{vals}        &#x9;

                        -}else{
                          tbody
                            input(type='text' id='tablename' name='tablename' value='#{tablenames[0]}' style='display:none;')
                            each item in val
                              input(type="text" id="id" name="id" value="#{Object.keys(item)[0]}" style='display:none;')
                              tr 
                                each vals,key in item 

                                  td #{vals}        &#x9;
                                td 
                                  input.btn.btn-primary.gg(type='submit' id='delete' name='#{item[Object.keys(item)[0]]}' value='DELETE')
                                  input.btn.btn-primary(type='submit' id='#{item[Object.keys(item)[0]]}' name='#{item[Object.keys(item)[0]]}' value='EDIT')
                        -}                                                                   
